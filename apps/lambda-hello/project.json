{
  "name": "lambda-hello",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/lambda-hello/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/lambda-hello",
        "main": "apps/lambda-hello/src/main.ts",
        "tsConfig": "apps/lambda-hello/tsconfig.app.json",
        "assets": [
          "apps/lambda-hello/src/assets"
        ],
        "compiler": "tsc",
        "target": "node",
        "webpackConfig": "apps/lambda-hello/webpack.config.ts"
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "lambda-hello:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "lambda-hello:build:development"
        },
        "production": {
          "buildTarget": "lambda-hello:build:production"
        }
      }
    }
  }
}
