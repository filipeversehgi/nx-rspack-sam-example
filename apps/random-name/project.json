{
  "name": "random-name",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/random-name/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/random-name",
        "tsConfig": "apps/random-name/tsconfig.app.json",
        "packageJson": "apps/random-name/package.json",
        "main": "apps/random-name/src/index.ts",
        "assets": [],
        "compiler": "tsc",
        "target": "node",
        "webpackConfig": "apps/random-name/webpack.config.ts"
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "random-name:build",
        "runBuildTargetDependencies": false,
        "watch": false,
        "port": 3001,
        "allowedHosts": ".ngrok-free.app"
      }
    }
  }
}
